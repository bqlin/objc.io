<span class=3D"kd">static</span> <span class=3D"kd">class</span> <span clas=
s=3D"nc">MyConfiguration</span> <span class=3D"o">{</span>
    <span class=3D"nd">@Bean</span>
    <span class=3D"kd">public</span> <span class=3D"n">Client</span> <span =
class=3D"nf">client</span><span class=3D"o">(</span><span class=3D"n">Servi=
ceExample</span> <span class=3D"n">service</span><span class=3D"o">)</span>=
 <span class=3D"o">{</span>
        <span class=3D"k">return</span> <span class=3D"k">new</span> <span =
class=3D"n">Client</span><span class=3D"o">(</span><span class=3D"n">servic=
e</span><span class=3D"o">);</span>
    <span class=3D"o">}</span>
<span class=3D"o">}</span>
</pre></div>
